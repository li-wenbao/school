<view class="list"><block wx:for="{{storeList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="flex justify-between padding-bottom-sm margin-sm" data-data="{{item}}" data-event-opts="{{[['tap',[['hanlderThe',['$event']]]]]}}" bindtap="__e"><view style="width:160rpx;height:160rpx;overflow:hidden;position:relative;"><block wx:if="{{item.storetype==0}}"><view class="rag text-white">已停业</view></block><image style="width:160rpx;height:160rpx;border-radius:8rpx;display:block;" mode="scaleToFill" lazy-load="true" src="{{imgurl+item.logoId+'.json'}}"></image></view><view style="width:510rpx;"><view class="padding-bottom-xs text-black text-bold text-xl">{{item.name}}</view><view class="flex justify-between padding-bottom-xs text-jiujiu"><view class="margin-top-xs"><text class="cuIcon-favorfill text-orange text-sm"></text><text class="text-sm">{{item.merchantStat}}</text><text class="margin-left-sm text-sm">{{"月销"+item.count}}</text></view></view><view class="padding-bottom-xs margin-top-xs solid-bottom text-sm"><text class="text-sm">起送</text><text class="text-price text-sm">{{item.initialDeliveryPrice}}</text><text class="margin-left-sm text-sm">配送</text><text class="text-price text-sm">{{item.distributionFee}}</text></view></view></view></block><block wx:if="{{loading}}"><view class="cu-load loading"></view></block><block wx:if="{{over}}"><view class="cu-load over"></view></block></view>