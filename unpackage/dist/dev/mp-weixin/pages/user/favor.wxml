<view class="page"><cu-custom vue-id="7baadf9a-1" bgColor="bg-blue" isBack="{{true}}" bind:__l="__l" vue-slots="{{['backText','content','right']}}"><view slot="backText">我的收藏</view><view slot="content"></view><view slot="right"><view class="action"></view></view></cu-custom><view><block wx:for="{{storeList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="flex margin-sm padding-sm radius bg-white radius20" style="position:relative;"><view class="margin-right-sm" style="width:160rpx;height:160rpx;overflow:hidden;"><image style="width:160rpx;height:160rpx;border-radius:8rpx;" mode="scaleToFill" lazy-load="true" src="{{item.imgUrl}}" data-id="{{item.id}}" data-event-opts="{{[['tap',[['_goToStore',['$event']]]]]}}" bindtap="__e"></image></view><view style="width:530rpx;"><view class="flex justify-between padding-bottom-sm"><view class="text-black text-bold text-lg" data-id="{{item.id}}" data-event-opts="{{[['tap',[['_goToStore',['$event']]]]]}}" bindtap="__e">{{item.name}}</view><view class="text-xxl" data-index="{{index}}" data-event-opts="{{[['tap',[['_showModal',['$event']]]]]}}" bindtap="__e"><text class="cuIcon-more text-grey"></text></view></view><view class="flex justify-between padding-bottom-sm text-grey"><view data-event-opts="{{[['tap',[['_goToStore',['$event']]]]]}}" bindtap="__e"><text class="cuIcon-favorfill text-orange"></text><text>{{item.grade}}</text><text class="margin-left-sm">{{"月销"+item.sales}}</text></view></view><view class="text-grey padding-bottom-sm" data-id="{{item.id}}" data-event-opts="{{[['tap',[['_goToStore',['$event']]]]]}}" bindtap="__e"><text>起送</text><text class="text-price">{{item.basePrice}}</text><text class="margin-left-sm">配送</text><text class="text-price">{{item.distribution}}</text><text class="margin-left-sm">人均</text><text class="text-price">{{item.averageConsume}}</text></view></view></view></block></view></view>