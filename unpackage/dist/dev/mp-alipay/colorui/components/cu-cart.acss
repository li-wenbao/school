
.cu-car {
	position: fixed;
	width: 100%;
	bottom: 0;
	min-height: 0px;
	-webkit-box-shadow: 0px 0px 0px;
	        box-shadow: 0px 0px 0px;
	z-index: 999;
}
.cu-tag.badge {
	border-radius: 50%;
	position: absolute;
	top: 10rpx;
	right: -5rpx;
	font-size: 20rpx;
	height: 30rpx;
	width: 30rpx;
	line-height: 30rpx;
	text-align: center;
	color: #ffffff;
	z-index: 1000;
}
.cu-tag.badge:not([class*="bg-"]) {
	background-color: #FA0000;
}

