<view class="list"><block a:for="{{storeList}}" a:for-item="item" a:for-index="index" a:key="index"><view class="flex justify-between padding-bottom-sm margin-sm" data-data="{{item}}" data-event-opts="{{[['tap',[['hanlderThe',['$event']]]]]}}" onTap="__e"><view style="width:160rpx;height:160rpx;overflow:hidden;position:relative;"><block a:if="{{item.storetype==0}}"><view class="rag text-white">已停业</view></block><image style="width:160rpx;height:160rpx;border-radius:8rpx;display:block;" mode="scaleToFill" lazy-load="true" src="{{item.url}}"></image></view><view style="width:510rpx;"><view class="padding-bottom-xs text-black text-bold text-xl">{{item.name}}</view><view class="flex justify-between padding-bottom-xs text-jiujiu"><view class="margin-top-xs"><text class="cuIcon-favorfill text-orange text-sm"></text><text class="text-sm">{{item.grade}}</text><text class="margin-left-sm text-sm">{{"月销"+item.sales}}</text></view></view><view class="padding-bottom-xs margin-top-xs solid-bottom text-sm"><text class="text-sm">起送</text><text class="text-price text-sm">{{item.baseprice}}</text><text class="margin-left-sm text-sm">配送</text><text class="text-price text-sm">{{item.distribution}}</text><text class="margin-left-sm text-sm">人均</text><text class="text-price text-sm">{{item.averageConsume}}</text></view></view></view></block><block a:if="{{loading}}"><view class="cu-load loading"></view></block><block a:if="{{over}}"><view class="cu-load over"></view></block></view>