.nav .cu-item {
	margin: 0 50rpx;
}
/* 重写btn 线框样式 */
.cu-btn[class*="line"]::after {
    content: " ";
    display: block;
    width: 200%;
    height: 200%;
    position: absolute;
    top: 0;
    left: 0;
    border: 0.5px solid currentColor;
    -webkit-transform: scale(0.5);
    transform: scale(0.5);
    -webkit-transform-origin: 0 0;
    transform-origin: 0 0;
    -webkit-box-sizing: border-box;
            box-sizing: border-box;
    border-radius:20px;
    z-index: 1;
    pointer-events: none;
}

