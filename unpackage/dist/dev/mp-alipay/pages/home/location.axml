<view class="page"><cu-custom vue-id="e60c40c0-1" bgColor="bg-blue" isBack="{{true}}" onVueInit="__l"><view slot="backText"></view><view slot="content"></view><view slot="right"><view class="action"></view></view></cu-custom><view><view class="bg-white cu-bar search"><navigator url="/pages/user/city" hover-class="none"><text class="cuIcon-location margin-left-sm"></text><text class="text-black padding-lr-xs">{{city}}</text><text class="cuIcon-unfold"></text></navigator><view class="search-form"><text class="cuIcon-search text-grey text-xxxl"></text><input type="text" placeholder="请输入收货地址" confirm-type="search"/></view></view><view class="bg-white padding-lr cu-bar solid-top"><block a:if="{{!isLocation}}"><view class="{{(('cu-load')+' '+(locationError?'erro over':'loading'))}}"></view></block><block a:else><view data-event-opts="{{[['tap',[['_chooseCurLocation',['$event']]]]]}}" class="text-black" onTap="__e">{{address}}</view></block><view data-event-opts="{{[['tap',[['_getCurLocation',['$event']]]]]}}" class="text-blue" onTap="__e"><text class="cuIcon-radiobox"></text><text class="margin-left-xs">重新定位</text></view></view><view class="bg-white padding margin-top-sm"><view class="padding-bottom-sm flex justify-between align-center"><view><text class="cuIcon-home text-gray margin-right-xs"></text><text class="text-gray">我的收货地址</text></view><navigator url="/pages/user/addAddress" hover-class="none"><text class="text-blue">管理</text></navigator></view><view class="padding-left margin-left-xs"><block a:for="{{myLocation}}" a:for-item="item" a:for-index="index" a:key="index"><view class="solid-bottom padding-tb-sm"><view><text class="margin-right-sm text-black">{{item.address}}</text><block a:if="{{item.tag!==''}}"><text class="cu-tag bg-olive light sm">{{item.tag}}</text></block></view><view class="text-sm margin-top-xs"><text class="margin-right">{{item.name}}</text><text>{{item.tel}}</text></view></view></block></view></view><view class="bg-white padding margin-top-sm"><view class="padding-bottom-sm"><text class="cuIcon-location text-gray margin-right-xs"></text><text class="text-gray">附近地址</text></view><view class="padding-left margin-left-xs"><block a:for="{{pois}}" a:for-item="item" a:for-index="index" a:key="index"><view class="solid-bottom padding-tb" data-id="{{index}}" data-event-opts="{{[['tap',[['_choosePoisLocation',['$event']]]]]}}" onTap="__e"><text class="margin-right-sm text-black">{{item.name}}</text></view></block></view></view></view></view>